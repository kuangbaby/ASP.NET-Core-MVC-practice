@model IEnumerable<Hellocnm.Models.读者分类>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DictName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LimitNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LimitDay)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DictName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LimitNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LimitDay)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.kind_num">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.kind_num">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.kind_num">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
